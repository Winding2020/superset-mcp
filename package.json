{
  "name": "superset-mcp",
  "version": "1.3.0",
  "description": "MCP server for reading and editing Apache Superset dataset data",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "superset-mcp": "build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
    "dev": "tsc --watch",
    "start": "node build/index.js",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "axios": "^1.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "files": [
    "build",
    "README.md",
    "README_zh.md"
  ],
  "keywords": [
    "mcp",
    "superset",
    "dataset",
    "apache-superset",
    "model-context-protocol"
  ],
  "homepage": "https://github.com/winding2020/superset-mcp",
  "author": "Winding2020",
  "license": "MIT"
}
